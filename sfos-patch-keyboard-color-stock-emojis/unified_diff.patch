+import QtQuick 2.0
+import Sailfish.Silica 1.0
+
+Item {
+    id: root
+    property Image icon
+    property bool enabled: true
+
+    Rectangle {
+        id: placeholder
+        anchors.fill: parent
+        radius: width
+        color: Theme.primaryColor
+        visible: enabled && icon.status !== Image.Ready &&
+                 icon.status !== Image.Loading // to avoid flickering
