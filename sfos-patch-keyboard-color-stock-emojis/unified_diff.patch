+// This file is part of sfos-patch-keyboard-color-stock-emojis
+// SPDX-FileCopyrightText: 2023 Mirian Margiani
+// SPDX-License-Identifier: GPL-3.0-or-later
+
+import QtQuick 2.0
+import io.thp.pyotherside 1.5
+import Nemo.Notifications 1.0
+
+Item {
+    id: tool
+
+    Notification {
+        id: noteStart
+        previewSummary: 'The keyboard is being restarted and should be back shortly.'
+        isTransient: true
+        icon: 'icon-lock-information'
+        appIcon: icon
+    }
+
+    Notification {
+        id: noteFail
+        previewSummary: 'Failed to restart the keyboard service.'
+        isTransient: true
+        icon: 'icon-lock-warning'
+        appIcon: icon
+    }
+
+    Python {
+        id: py
+
+        onError: {
+            noteFail.publish()
+            console.error('failed to restart maliit-server:', traceback)
+        }
+
+        Component.onCompleted: {
+            noteStart.publish()
+            importModule('subprocess', run_systemctl)
+        }
+    }
+
+    function run_systemctl() {
+        py.call('subprocess.run', [['systemctl', '--user', 'restart', 'maliit-server']], handle_systemctl)
+    }
+
+    function handle_systemctl(success) {
+        // Note: we don't handle this because Python errors are handled
+        // in the error handler and the item is destroyed on success anyway.
+    }
+}
